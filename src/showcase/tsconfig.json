{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": ["es2018", "dom"],
    "types": ["arcgis-js-api", "grecaptcha"],
    "paths": {
      "@sbb-esta/angular-business": ["../angular-business"],
      "@sbb-esta/angular-business/*": ["../angular-business/*"],
      "@sbb-esta/angular-core": ["../angular-core"],
      "@sbb-esta/angular-core/*": ["../angular-core/*"],
      "@sbb-esta/angular-icons": ["../angular-icons"],
      "@sbb-esta/angular-icons/*": ["../angular-icons/*"],
      "@sbb-esta/angular-keycloak": ["../angular-keycloak"],
      "@sbb-esta/angular-keycloak/*": ["../angular-keycloak/*"],
      "@sbb-esta/angular-leaflet": ["../angular-leaflet"],
      "@sbb-esta/angular-leaflet/*": ["../angular-leaflet/*"],
      "@sbb-esta/angular-maps": ["../angular-maps"],
      "@sbb-esta/angular-maps/*": ["../angular-maps/*"],
      "@sbb-esta/angular-public": ["../angular-public"],
      "@sbb-esta/angular-public/*": ["../angular-public/*"]
    }
  },
  "bazelOptions": {
    // See https://github.com/angular/angular/issues/29107
    "devmodeTargetOverride": "es5"
  }
}
